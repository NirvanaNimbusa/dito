{
  "sourceType": "module",
  "sourceMaps": "both",
  "presets": [
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "corejs": 3
    }],
    "@ditojs/babel-preset"
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "alias": {
        "~": ".",
        "@": "./src"
      }
    }]
  ],
  "comments": false
}
