{
  "sourceType": "module",
  "sourceMaps": "both",
  "presets": [
    "@ditojs/babel-preset",
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "corejs": 3
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "cwd": "babelrc",
      "alias": {
        "~": ".",
        "@": "./src"
      }
    }],
    "dynamic-import-node",
    "transform-promise-to-bluebird"
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }]
      ]
    }
  },
  "comments": false
}
